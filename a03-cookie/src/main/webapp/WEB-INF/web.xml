<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CookieServlet</servlet-name>
  	<display-name>CookieServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.CookieServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CookieServlet2</servlet-name>
  	<display-name>CookieServlet2</display-name>
  	<description></description>
  	<servlet-class>servlet.CookieServlet2</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>CookieServlet</servlet-name>
  	<url-pattern>/CookieServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CookieServlet2</servlet-name>
  	<url-pattern>/CookieServlet2</url-pattern>
  </servlet-mapping>
</web-app>
